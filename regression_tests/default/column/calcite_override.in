#Description: 1D Calcite dissolution problem

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
    /
  /
END

SUBSURFACE

EXTERNAL_FILE calcite_base.txt

CHEMISTRY
  EXTERNAL_FILE calcite_chem_base.txt
  MINERAL_KINETICS
    Calcite
      RATE_CONSTANT 1.d-6
    /
  /
  OVERRIDE_MINERAL_MASS_ACTION
    Calcite
      REACTION Calcite + H+ <-> Ca++ + 1. HCO3-
      LOGK 2.2257 1.8487 1.3330 0.7743 0.0999 -0.5838 -1.3262 -2.2154
    /
  /
END

TRANSPORT_CONDITION inlet
  TYPE DIRICHLET_ZERO_GRADIENT
  TIME_UNITS y
  CONSTRAINT_LIST
    0.d0 undersaturated
    10.d0 supersaturated
  /
END

END_SUBSURFACE
