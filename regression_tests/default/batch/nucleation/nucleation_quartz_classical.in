SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
      OPTIONS
      /
    /
  /
END

SUBSURFACE

  EXTERNAL_FILE nucleation_quartz.txt

#=========================== chemistry ========================================
CHEMISTRY
  EXTERNAL_FILE nucleation_quartz_chem.txt
  MINERAL_KINETICS
    Quartz
      RATE_CONSTANT 0. mol/m^2-sec
      NUCLEATION_KINETICS classical
    /
  /
  NUCLEATION_KINETICS
    CLASSICAL classical
      RATE_CONSTANT 1.d-5
      GEOMETRIC_SHAPE_FACTOR 0.65
      HETEROGENEOUS_CORRECTION_FACTOR 0.15
      SURFACE_TENSION 0.685
    /
  /
END

END_SUBSURFACE
